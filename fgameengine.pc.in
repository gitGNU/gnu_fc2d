prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
datarootdir=@datarootdir@

Name: FGameEngine
Description: Complete tool kit for 3D games development.
Version: @FGE_REAL_VERSION@
Libs: -L${libdir} -lfgameengine-@FGE_VERSION@  \
	@GLIB_LIBS@ \
	@PYTHON_LIBS@ \
	@GL_LIBS@ \
	@SDL_LIBS@ \
	@GTHREAD_LIBS@

Cflags: -I${includedir}/fgameengine-@FGE_VERSION@ \
	@GLIB_CFLAGS@ \
	@PYTHON_CFLAGS@ \
	@GL_CFLAGS@ \
	@SDL_CFLAGS@ \
	@GTHREAD_CFLAGS@